{
	"name": "minicode",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"bun run dev:frontend\" \"bun run dev:server\"",
		"dev:frontend": "vite dev --port 3000",
		"dev:server": "bun run --watch server/index.ts",
		"build": "vite build && bun build server/index.ts --outdir dist/server --target bun",
		"preview": "vite preview",
		"start:server": "bun run dist/server/index.js",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio"
	},
	"dependencies": {
		"@base-ui/react": "^1.1.0",
		"@fontsource-variable/jetbrains-mono": "^5.2.8",
		"@monaco-editor/react": "^4.7.0",
		"@phosphor-icons/react": "^2.1.10",
		"@tailwindcss/postcss": "^4.1.18",
		"@tanstack/react-devtools": "^0.7.0",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-router-devtools": "^1.132.0",
		"@tanstack/react-router-ssr-query": "^1.131.7",
		"@tanstack/react-start": "^1.132.0",
		"@tanstack/router-plugin": "^1.132.0",
		"better-auth": "^1.4.16",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.45.1",
		"elysia": "^1.2.25",
		"lucide-react": "^0.561.0",
		"nitro": "npm:nitro-nightly@latest",
		"pg": "^8.17.2",
		"postcss": "^8.5.6",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"shadcn": "^3.7.0",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"vite-tsconfig-paths": "^6.0.2",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@tanstack/devtools-vite": "^0.3.11",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.2.0",
		"@types/bun": "latest",
		"@types/node": "^22.10.2",
		"@types/pg": "^8.16.0",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^5.0.4",
		"concurrently": "^9.1.2",
		"drizzle-kit": "^0.31.8",
		"jsdom": "^27.0.0",
		"tsx": "^4.21.0",
		"typescript": "^5.7.2",
		"vite": "^7.1.7",
		"vitest": "^3.0.5",
		"web-vitals": "^5.1.0"
	}
}
